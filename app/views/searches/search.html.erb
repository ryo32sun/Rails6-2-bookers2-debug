<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-8 offset-md-1'>
      <% if @model == "User" %>
        <h2>Users</h2>
        <%= render "users/index", users: @users %>
      <% else %>
        <h2>Books</h2>
        <%= render 'books/index', books: @books %>
      <% end %>
    </div>
  </div>
</div>